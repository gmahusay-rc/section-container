{
  "name": "hero-banner",
  "version": "0.1.0",
  "private": true,
  "main": "dist/SectionContainer.js",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:npm": "rm -rf dist && NODE_ENV=production babel src/component/SectionContainer --out-dir dist --copy-files",
    "tailwind:SectionContainer:build": "npx tailwindcss build ./src/css/SectionContainer.css -o ./src/component/SectionContainer/css/SectionContainer.css",
    "tailwind:SectionContainer:watch": "npx tailwindcss --watch ./src/css/SectionContainer.css -o ./src/component/SectionContainer/css/SectionContainer.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5"
  }
}
